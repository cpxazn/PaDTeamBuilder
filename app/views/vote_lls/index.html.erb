<p id="notice"><%= notice %></p>

<h1>Listing Vote Lls</h1>

<table>
  <thead>
    <tr>
      <th>Leaders</th>
      <th>User</th>
      <th>Score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vote_lls.each do |vote_ll| %>
      <tr>
        <td><%= vote_ll.leaders %></td>
        <td><%= vote_ll.user_id %></td>
        <td><%= vote_ll.score %></td>
        <td><%= link_to 'Show', vote_ll %></td>
        <td><%= link_to 'Edit', edit_vote_ll_path(vote_ll) %></td>
        <td><%= link_to 'Destroy', vote_ll, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote ll', new_vote_ll_path %>
